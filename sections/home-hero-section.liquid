                          <linearGradient id="paint0_linear_295_199" x1="91.8335" y1="11" x2="113.833" y2="11" gradientUnits="userSpaceOnUse">
                          <stop offset="0.5"/>
                          <stop offset="0.5" stop-color="#D1D1D1"/>
                          </linearGradient>
                          </defs>
                          </svg>

                        </div>
                        <div class="b-hero__trustreview-text underline-text">{{ trustReview }}</div>
                        
                        
                    </div>
                {% endif %}
        
                <h1 class="b-hero__title">{{ sectionTitle }}</h1>
                <p class="b-hero__subtitle">{{ sectionSubtitle }}</p>
        
                 <div class="b-hero__bullets-container">
                    {% for block in section.blocks %}
                         {% liquid  
                            assign bulletIcon = block.settings.hero_bullet_icon 
                            assign bulletText = block.settings.hero_bullet_text
                        %}
                        <div class="b-hero__bullet-container">
                            <div class="b-hero__bullet-icon">
                                {{ bulletIcon | image_url: width: auto | image_tag: class: 'b-hero__bullet-icon-img'}}
                            </div>
                            <div class="b-hero__bullet-text">{{ bulletText }}</div>
                        </div>
                    {% endfor %}
                 </div>
        
                 {% if heroButtonLink != blank %}
                      <div class="b-hero__button-wrapper pdp-btn--wrapper"> 
                        <div class="b-hero__button">
                        <a
                          href="{{ heroButtonLink }}"
                          title="{{ heroButtonText }}"
                          class="b-hero-btn b-hero--main pdp-btn">{{ heroButtonText }}</a>
                      </div>
                      </div>
                 {% endif %}
        
                <div class="b-hero-review__box-container">
                    <div class="b-hero-review__box-container-header">
                        <div class="b-review__author">
                            {% if heroReviewAuthor %}
                                {{ heroReviewAuthor | image_url: width: auto | image_tag: class: 'b-review__author-img'}}
                            {% endif %}
                                <div class="b-hero-review__box-header-left">
                                    <div class="b-hero-review__author-name">{{ heroReviewAuthorName }}</div>
                                    <div class="b-hero-reviews__box-header-bottom">
                                        <span>{{ heroReviewNumber }}</span>
                                        <span>
                                            <svg width="14" height="14" viewBox="0 0 14 14" fill="none" xmlns="http://www.w3.org/2000/svg">
                                            <path fill-rule="evenodd" clip-rule="evenodd" d="M7 12.3598L7.42067 11.8858C7.898 11.3396 8.32756 10.8198 8.70933 10.3265L9.02467 9.9105C10.3416 8.13673 11 6.72939 11 5.6885C11 3.46717 9.20933 1.6665 7 1.6665C4.79067 1.6665 3 3.46717 3 5.6885C3 6.72939 3.65844 8.13695 4.97533 9.91117L5.29067 10.3272C5.8357 11.0254 6.40585 11.7029 7 12.3598Z" stroke="black" stroke-width="0.666667" stroke-linecap="round" stroke-linejoin="round"/>
