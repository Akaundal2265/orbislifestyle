{{ 'home-hero.css' | asset_url | stylesheet_tag }}

{% liquid
    assign obj = section.settings
    assign trustReview = obj.trustpilot_text 
    assign sectionTitle = obj.hero_title
    assign sectionSubtitle = obj.hero_subtitle
    assign heroBadge = obj.hero_badge
    assign heroBgImage = obj.hero_bg_image
    assign heroMobImage = obj.hero_image
    assign heroButtonLink = obj.hero_button_url
    assign heroButtonText = obj.hero_button_text
    assign heroReviewAuthor = obj.hero_review_author_image
    assign heroReviewAuthorName = obj.hero_review_author
    assign heroReviewTitle = obj.hero_review_title
    assign heroReviewText = obj.hero_review_content
    assign heroAuthorImage = obj.hero_author_image
    assign heroReviewNumber = obj.hero_review_number
    assign heroReviewLocation = obj.hero_review_location
    assign heroReviewDate = obj.hero_review_date
    assign heroReviewExperiance = obj.hero_review_experiance

%}

<section class="b-hero product-section">
    <div class="l">
        <div class="b-hero__wrapper">
            <div class="b-hero__left-container">
                {% if trustReview %}
                    <div class="b-hero__trustreview">
                        <div class="b-hero__trustreview-stars">
                        <svg width="114" height="22" viewBox="0 0 114 22" fill="none" xmlns="http://www.w3.org/2000/svg">
